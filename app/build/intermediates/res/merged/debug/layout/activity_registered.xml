<?xml version="1.0" encoding="utf-8"?>
<cn.gavinliu.android.lib.scale.ScaleRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    android:layout_gravity="center"
    android:orientation="vertical">


    <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
        android:id="@+id/relativeLayout12"
        android:layout_width="320dp"
        android:layout_height="100dp"
        android:layout_below="@+id/relativeLayout11"
        android:layout_centerHorizontal="true"
        android:background="@drawable/registered_01">

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true">

            <EditText
                android:id="@+id/editText"
                android:layout_width="260dp"
                android:layout_height="30dp"
                android:layout_alignLeft="@+id/editText3"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/editText3"
                android:background="@drawable/edittext"
                android:digits="1234567890qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM"
                android:ems="10"
                android:hint="六位以上英文數字"
                android:inputType="text"
                android:textSize="15sp"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/editText"
                android:layout_alignBottom="@+id/editText"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:gravity="left"
                android:textSize="12sp" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true">

            <EditText
                android:id="@+id/editText3"
                android:layout_width="260dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:background="@drawable/edittext"
                android:ems="10"
                android:hint="請至少輸入6碼"
                android:inputType="textPassword"
                android:textSize="15sp"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/editText3"
                android:layout_alignBottom="@+id/editText3"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:gravity="left"
                android:textSize="12sp" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="225dp"
            android:layout_height="30dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true">

            <EditText
                android:id="@+id/editText5"
                android:layout_width="225dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_weight="1"
                android:background="@drawable/edittext"
                android:ems="10"
                android:hint="請在輸入一次密碼"
                android:inputType="textPassword"
                android:textSize="15sp"
                tools:ignore="HardcodedText,ObsoleteLayoutParam" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="left"
                android:textSize="12sp" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
        android:id="@+id/relativeLayout14"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignEnd="@+id/relativeLayout13"
        android:layout_alignLeft="@+id/relativeLayout13"
        android:layout_alignRight="@+id/relativeLayout13"
        android:layout_alignStart="@+id/relativeLayout13"
        android:layout_below="@+id/relativeLayout13"
        android:layout_marginTop="14dp"
        android:background="@drawable/registered_03">

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            tools:ignore="UselessParent">

            <EditText
                android:id="@+id/editText6"
                android:layout_width="260dp"
                android:layout_height="35dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_weight="1"
                android:background="@drawable/edittext"
                android:ems="10"
                android:hint="請輸入電話"
                android:inputType="text|phone"
                android:textSize="15sp"
                tools:ignore="HardcodedText,ObsoleteLayoutParam" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="left"
                android:textSize="12sp" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
        android:id="@+id/relativeLayout11"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/relativeLayout12"
        android:layout_alignLeft="@+id/relativeLayout12"
        android:layout_alignRight="@+id/relativeLayout12"
        android:layout_alignStart="@+id/relativeLayout12">

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/icon_longin" />

    </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
        android:id="@+id/relativeLayout13"
        android:layout_width="100dp"
        android:layout_height="115dp"
        android:layout_alignEnd="@+id/relativeLayout12"
        android:layout_alignLeft="@+id/relativeLayout12"
        android:layout_alignRight="@+id/relativeLayout12"
        android:layout_alignStart="@+id/relativeLayout12"
        android:layout_below="@+id/relativeLayout12"
        android:layout_marginTop="13dp"
        android:background="@drawable/registered_02">

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="260dp"
            android:layout_height="35dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true">

            <EditText
                android:id="@+id/editText4"
                android:layout_width="260dp"
                android:layout_height="35dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_weight="1.01"
                android:background="@drawable/edittext"
                android:ems="10"
                android:hint="姓名與提領帳戶名相同"
                android:inputType="textPersonName"
                android:textSize="15sp"
                tools:ignore="HardcodedText,ObsoleteLayoutParam" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="left"
                android:textSize="12sp" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true">

            <EditText
                android:id="@+id/editText2"
                android:layout_width="260dp"
                android:layout_height="30dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_weight="1"
                android:background="@drawable/edittext"
                android:ems="10"
                android:hint="請輸入暱稱"
                android:inputType="text"
                android:textSize="15sp"
                tools:ignore="HardcodedText,ObsoleteLayoutParam" />
        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true">

            <EditText
                android:id="@+id/editText7"
                android:layout_width="260dp"
                android:layout_height="30dp"
                android:layout_alignLeft="@+id/editText2"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignStart="@+id/editText2"
                android:layout_weight="1"
                android:background="@drawable/edittext"
                android:ems="10"
                android:hint="請輸入信箱"
                android:inputType="text"
                android:textSize="15sp"
                tools:ignore="HardcodedText,ObsoleteLayoutParam" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="left"
                android:textSize="12sp" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/relativeLayout14"
        android:layout_alignLeft="@+id/relativeLayout14"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/relativeLayout14"
        android:layout_alignStart="@+id/relativeLayout14"
        android:layout_below="@+id/relativeLayout20">


        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/prediction">

            <Button
                android:id="@+id/button3"
                android:layout_width="match_parent"
                android:layout_height="40dp"

                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="false"
                android:layout_marginTop="10dp"
                android:background="@drawable/registered_07" />
        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/prediction">

            <Button
                android:id="@+id/button6"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/prediction"
                android:layout_marginTop="10dp"
                android:background="@drawable/registered_06" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <Button
            android:id="@+id/prediction"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/checkBox2"
            android:layout_weight="1"
            android:background="@drawable/registered_05"
            tools:ignore="ObsoleteLayoutParam,UnknownIdInLayout" />

    </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

    <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
        android:id="@+id/relativeLayout20"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignEnd="@+id/relativeLayout14"
        android:layout_alignLeft="@+id/relativeLayout14"
        android:layout_alignRight="@+id/relativeLayout14"
        android:layout_alignStart="@+id/relativeLayout14"
        android:layout_below="@+id/relativeLayout14">

        <Button
            android:id="@+id/button5"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/textView2"
            android:layout_centerHorizontal="true"
            android:background="@drawable/registered_08" />

        <CheckBox
            android:id="@+id/checkBox"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/checkBox"
            android:layout_toRightOf="@+id/checkBox"
            android:text="我同意會員條款"
            tools:ignore="HardcodedText" />
    </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

</cn.gavinliu.android.lib.scale.ScaleRelativeLayout>