<?xml version="1.0" encoding="utf-8"?>
<cn.gavinliu.android.lib.scale.ScaleRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    android:layout_gravity="center"
    tools:context="com.example.user.sport58.login">


        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar2"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="#FFC125"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:id="@+id/relativeLayout9"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/relativeLayout3">

                <TextView
                    android:layout_width="330dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/other_login" />
        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_alignEnd="@+id/relativeLayout3"
            android:layout_alignLeft="@+id/relativeLayout3"
            android:layout_alignRight="@+id/relativeLayout3"
            android:layout_alignStart="@+id/relativeLayout3"
            android:layout_below="@+id/relativeLayout9">

                <com.facebook.login.widget.LoginButton
                    android:id="@+id/fb_login_bn"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button9"
                    android:layout_marginTop="12dp" />

                <Button
                    android:id="@+id/button9"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/google_login" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <TextView
            android:layout_width="105dp"
            android:layout_height="20dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/copyrighyt" />


        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="300dp"
            android:layout_height="150dp"
            android:layout_alignLeft="@+id/relativeLayout2"
            android:layout_alignStart="@+id/relativeLayout2"
            android:layout_below="@+id/toolbar2">

                <ImageView
                    android:layout_width="300dp"
                    android:layout_height="150dp"
                    android:layout_alignParentTop="true"
                    app:srcCompat="@drawable/icon_longin"
                    tools:ignore="ContentDescription" />

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="300dp"
            android:layout_height="110dp"
            android:layout_below="@+id/relativeLayout"
            android:layout_centerHorizontal="true">

                <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/textView11"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true">

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="300dp"
                            android:layout_height="50dp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:background="@drawable/account_login" />

                        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true">

                                <EditText
                                    android:id="@+id/editText9"
                                    android:layout_width="250dp"
                                    android:layout_height="25dp"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentRight="true"
                                    android:layout_alignParentTop="true"
                                    android:layout_marginTop="14dp"
                                    android:background="@drawable/edittext"
                                    android:ems="10"
                                    android:inputType="text"
                                    android:textSize="15sp" />
                        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

                </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

                <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
                    android:id="@+id/relativeLayout7"
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignTop="@+id/textView10">

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="300dp"
                            android:layout_height="50dp"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/password_login" />

                        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignTop="@+id/textView10">

                                <EditText
                                    android:id="@+id/editText10"
                                    android:layout_width="250dp"
                                    android:layout_height="25dp"
                                    android:layout_alignParentEnd="true"
                                    android:layout_alignParentRight="true"
                                    android:layout_alignTop="@+id/relativeLayout7"
                                    android:layout_marginTop="14dp"
                                    android:background="@drawable/edittext"
                                    android:ems="10"
                                    android:inputType="textPassword"
                                    android:textSize="15sp" />
                        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

                </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
            android:id="@+id/relativeLayout3"
            android:layout_width="300dp"
            android:layout_height="70dp"
            android:layout_alignLeft="@+id/relativeLayout2"
            android:layout_alignStart="@+id/relativeLayout2"
            android:layout_below="@+id/relativeLayout2">

                <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
                    android:layout_width="145dp"

                    android:layout_height="50dp"
                    android:layout_alignBaseline="@+id/relativeLayout5"
                    android:layout_alignBottom="@+id/relativeLayout5"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">

                        <Button
                            android:id="@+id/button8"
                            android:layout_width="150dp"
                            android:layout_height="50dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:background="@drawable/login_in" />

                </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

                <cn.gavinliu.android.lib.scale.ScaleRelativeLayout
                    android:id="@+id/relativeLayout5"
                    android:layout_width="145dp"
                    android:layout_height="50dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true">

                        <Button
                            android:id="@+id/button7"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:background="@drawable/login_registered" />

                </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        </cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

        <ProgressBar
            android:id="@+id/progressBar4"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/relativeLayout"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true" />

</cn.gavinliu.android.lib.scale.ScaleRelativeLayout>

